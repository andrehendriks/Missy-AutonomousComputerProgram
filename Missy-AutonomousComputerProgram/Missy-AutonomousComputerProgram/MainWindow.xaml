<Window x:Class="Missy_AutonomousComputerProgram.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Missy_AutonomousComputerProgram"
        mc:Ignorable="d"
        Title="Missy The Autonomous Computer Program" Height="350" Width="525" Loaded="Window_Loaded">
    <Grid Background="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}">
        <TextBlock x:Name="textBlock" Margin="10,10,10,59" TextWrapping="Wrap"/>
        <TextBox x:Name="textBox" Margin="10,0,100,10" TextWrapping="Wrap" KeyDown="textBox_KeyDown" Height="23" VerticalAlignment="Bottom"/>
        <Button x:Name="button" Content="Enter" Margin="0,0,10,10" Click="button_Click" HorizontalAlignment="Right" Width="85" Height="23" VerticalAlignment="Bottom"/>
    </Grid>
</Window>
